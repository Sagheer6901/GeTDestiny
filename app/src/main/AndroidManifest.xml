        <activity android:name=".Activity_lhr_details" />
        <activity
            android:name=".Activity_sign_in"
            android:label="@string/title_activity_login" />
        <activity android:name=".Activity_sign_up" />
        <activity android:name=".Activity_Selection_guest" />
        <activity android:name=".Activity_selection_adminorgeneral" />
        <activity android:name=".Activity_via_general" />
        <activity android:name=".Activity_via_guest" />
        <activity android:name=".Activity_available_twoseater" />
        <activity android:name=".Activity_sleeper_coach" />
